{
  "name": "sketch-threejs-v2",
  "version": "0.0.1",
  "description": "Three Js sketches",
  "main": "dist/index.js",
  "module": "dist/sketch-threejs-v2.esm.js",
  "jsnext:main": "dist/sketch-threejs-v2.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-s clean compile bundle",
    "bundle": "rollup -c",
    "check:format": "prettier --list-different \"**/*.{js,ts,tsx}\"",
    "check:types": "tsc --noEmit",
    "clean": "run-p clean:*",
    "clean:compiled": "shx rm -rf compiled",
    "clean:coverage": "shx rm -rf coverage",
    "clean:dist": "shx rm -rf dist",
    "compile": "tsc -p tsconfig.base.json",
    "format": "prettier --write \"**/*.{js,ts,tsx}\"",
    "lint": "eslint . --ext .js,.ts",
    "postbundle": "npm run clean:compiled && shx cp ./index.js ./dist/index.js",
    "release": "tanem-scripts release",
    "test": "run-s check:* lint build test:*",
    "test:cjs": "jest --config ./scripts/jest/config.cjs.js",
    "test:cjsprod": "cross-env NODE_ENV=production jest --config ./scripts/jest/config.cjs.js",
    "test:es": "jest --config ./scripts/jest/config.es.js",
    "test:src": "jest --config ./scripts/jest/config.src.js",
    "test:umd": "jest --config ./scripts/jest/config.umd.js",
    "test:umdprod": "jest --config ./scripts/jest/config.umdprod.js"
  },
  "repository": {
    "type": "git",
    "url": "github:jmikedupont2/sketch-threejs-v2"
  },
  "keywords": [
    "javascript",
    "react",
    "typescript"
  ],
  "author": "jmikedupont2 <jmikedupont2@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "github:jmikedupont2/sketch-threejs-v2/issues"
  },
  "homepage": "github:jmikedupont2/sketch-threejs-v2",
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "@deskpro/js-utils": "^1.0.13",
    "babel-loader": "^8.2.2",
    "beefy": "^2.1.8",
    "browserfy": "^1.0.0",
    "prop-types": "^15.7.2",
    "three": "^0.130.1",
    "ts-debounce": "^3.0.0",
    "tslib": "^2.3.0",
    "watchify": "^4.0.0",
    "webpack": "^5.10.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/runtime": "^7.14.6",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.0.0",
    "@testing-library/user-event": "13.1.9",
    "@types/jest": "26.0.24",
    "@types/prop-types": "15.7.4",
    "@types/react": "17.0.14",
    "@types/react-dom": "17.0.9",
    "@types/three": "^0.130.0",
    "@typescript-eslint/eslint-plugin": "4.28.3",
    "@typescript-eslint/parser": "4.28.3",
    "babel-core": "6.26.3",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "cross-env": "7.0.3",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "jest": "27.0.6",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-test-renderer": "17.0.2",
    "regenerator-runtime": "0.13.7",
    "rollup": "2.53.2",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-filesize": "9.1.1",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-size-snapshot": "0.12.0",
    "rollup-plugin-sourcemaps": "0.6.3",
    "rollup-plugin-terser": "7.0.2",
    "shx": "0.3.3",
    "tanem-scripts": "6.0.23",
    "ts-jest": "27.0.3",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.5",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.2"
  }
}
